    / HERO SECTION **************************************
    #hero.section
      .container

        .heroBox
          .heroBox__date
            .heroBox__date--line
            .heroBox__date--text 26 & 27 November
          .heroBox__heading
            .heroBox__wrap
              Welcome
              .heroBox__heading--line
            %strong Shift Money
          .heroBox__text
            The biggest Developer Conference in Southeast Europe. This event, Shift 2019 is the eight and will bring together more than 1300 attendees. During the two conference days, Shift becomes a meeting place for developers in the most beautiful city in the world.
          .heroBox__buttons
            %a.btn.green.empty(href="#tickets") Get Tickets
            .heroBox__buttons-spacer or
            %a.btn.link.white(href="https://www.youtube.com/watch?time_continue=1&v=YOMcSbT82Ks" target="_blenk") Watch our Video Review
            /.heroBox__buttons-spacer or
            /%a.btn.link.white(href="https://play.rtl.hr/digitalshapers/uzivo" target="_blenk") Watch Live Stream



    / ABOUT SECTION **************************************
    #about.section
      .container
        %h1.small
          Why Shift Money

        %p The biggest Developer Conference in Southeast Europe. This event, Shift 2019 is the eight and will bring together more than 1300 attendees. During the two conference days, Shift becomes a meeting place for developers in the most beautiful city in the world.




    / TICKETS SECTION **************************************
    #tickets.section
      .container
        %h1 Tickets
        .title-desc Buy tickets

        .tickets
          %iframe(allowtransparency="true" frameborder="0" height="360" hspace="0" marginheight="5" marginwidth="5" scrolling="auto" src="https://eventbrite.com/tickets-external?eid=47985670432&ref=etckt" vspace="0" width="100%")
