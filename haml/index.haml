    #hero.section
      .container

        .heroBox
          .heroBox__date
            .heroBox__date--text 26 & 27 November
            .heroBox__date--line
            .heroBox__date--text Zagreb, Croatia
          .heroBox__heading
            %strong Shift Money
            %br
            FinTech Conference
          .heroBox__buttons
            %a.btn.blue.empty(href="#tickets") Get Tickets
            .heroBox__buttons-spacer or
            %a.btn.link.white(href="https://www.youtube.com/watch?time_continue=1&v=YOMcSbT82Ks" target="_blenk") Watch our Video Review
            /.heroBox__buttons-spacer or
            /%a.btn.link.white(href="https://play.rtl.hr/digitalshapers/uzivo" target="_blenk") Watch Live Stream

        .heroDate
          26 & 27 November 2018



    / TICKETS SECTION **************************************
    #tickets.section
      .container
        %h1 Tickets
        .title-desc Buy tickets

        .tickets
          %iframe(src="https://www.eventbrite.com/tickets-external?eid=35075304179&ref=etckt" frameborder="0" height="531" width="100%" vspace="0" hspace="0" marginheight="5" marginwidth="5" scrolling="auto" allowtransparency="true")






