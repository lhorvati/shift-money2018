    / LOCATION SECTION **************************************
    #location.section
      .container
        %h1 Location
        .title-desc Where you can find us?

        .location
          .location__map
            / Google Maps
            #map(style="height: 100%;")
            :javascript
              function initMap() {
                var shiftSpot = {lat: 45.811607, lng: 15.940597};
                var shiftParty1 = {lat: 45.810168, lng: 15.942291};

                var map = new google.maps.Map(document.getElementById('map'), {
                  zoom: 16,
                  center: shiftSpot
                });

                var contentString = "<strong>Shift Money - FinTech Conference 2018</strong><br>Prilaz Baruna Filipovića 23, Zagreb<br>";

                var infowindow = new google.maps.InfoWindow({
                  content: contentString
                });

                var marker = new google.maps.Marker({
                  position: shiftSpot,
                  map: map,
                  title: 'Shift Money - FinTech Conference 2018'
                });



                marker.addListener('click', function() {
                  infowindow.open(map, marker);
                });
                infowindow.open(map, marker);
              }
            %script(async="" defer="defer" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCMAYab_tmsuABPTE_haSUcVBUqXIuuz5o&callback=initMap")

          .location__details
            %h2.small
              %span Shift Money
              FinTech Conference 2018
            .title-desc
              Prilaz Baruna Filipovića 23, 
              %br/
              Zagreb, Croatia


            .acommodation
